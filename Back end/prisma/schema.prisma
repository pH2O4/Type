// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  Id    Int    @default(autoincrement())
  Name  String
  Email String @unique
  Contato String 
  Senha String @default("123")
}
